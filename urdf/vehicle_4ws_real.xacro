<?xml version="1.0" ?>
<robot name="vehicle_4ws" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:property name="PI" value="3.1415926535897932384626433832795028841971" />
<xacro:property name="gravitational_acceleration" value="9.8" />
<xacro:property name="vehicle1_Yaw" value="-PI/4" />

<!-- 荷台と連結点のmass -->
<xacro:property name="fusion_vehicle_body_mass" value="2113.272311120373" /> 
<xacro:property name="vehicle_body_mass" value="313.272311120373" /> 
<xacro:property name="vehicle_inertia_coef" value="${vehicle_body_mass/fusion_vehicle_body_mass}" /> 

<xacro:property name="wheel_mass" value="4.639715839283939" /> 

<!-- frontは受動車輪なので低めに設定 -->
<xacro:property name="front_wheel_joint_damping" value="0.1"/>
<xacro:property name="front_wheel_joint_mu" value="0.001" />
<xacro:property name="front_wheel_joint_friction" value="${front_wheel_joint_mu* gravitational_acceleration * wheel_mass  }" /> 
<xacro:property name="front_steering_damping" value="5.0" />
<xacro:property name="front_steering_mu" value="0.001" />
<xacro:property name="front_steering_friction" value="${front_steering_mu * gravitational_acceleration * 0.25*vehicle_body_mass}" />

<!-- rearは駆動輪なので、入力以外で動かないように設定 -->
<xacro:property name="wheel_joint_damping" value="0.2"/>
<xacro:property name="wheel_joint_mu" value="0.05" />
<xacro:property name="wheel_joint_friction" value="${wheel_joint_mu* gravitational_acceleration * wheel_mass }" /> 
<xacro:property name="steering_damping" value="20.0" />
<xacro:property name="steering_mu" value="0.01" />
<xacro:property name="steering_friction" value="${steering_mu * gravitational_acceleration * 0.25*vehicle_body_mass}" />

<!-- wheelの接地面の摩擦などを定義 -->
<xacro:property name="wheel_surface_mu1" value="5.0"/>
<xacro:property name="wheel_surface_mu2" value="5.0" />
<xacro:property name="wheel_kp" value="1e5" /> 
<xacro:property name="wheel_kd" value="1.0" />
<xacro:property name="wheel_minDepth" value="0.01" />
<xacro:property name="wheel_maxVel" value="0.1" />

<!-- rotationは共通 -->
<xacro:property name="rotational_joint_damping" value="0.01" />
<xacro:property name="rotational_joint_mu" value="0.001" />
<xacro:property name="rotational_joint_friction" value="${rotational_joint_mu * gravitational_acceleration}" />

<xacro:include filename="$(find vehicle_4ws_description)/urdf/materials.xacro" />
<xacro:include filename="$(find vehicle_4ws_description)/urdf/vehicle_4ws.trans" />
<xacro:include filename="$(find vehicle_4ws_description)/urdf/vehicle_4ws.gazebo" />

<link name="base_link"/>

<link name="body_link">
  <inertial>
    <origin xyz="0.001411408730126027 0.05751189617802702 0.2" rpy="0 0 0"/>
    <mass value="${vehicle_body_mass}"/>
    <inertia ixx="${vehicle_inertia_coef*114.688091}" iyy="${vehicle_inertia_coef*336.418835}" izz="${vehicle_inertia_coef*418.647558}" ixy="${vehicle_inertia_coef*0.010139}" iyz="${vehicle_inertia_coef*0.002956}" ixz="${-vehicle_inertia_coef*0.33997}"/>
  </inertial>
  <visual>
    <origin xyz="0 0 -0.376" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://vehicle_4ws_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="_"/>
  </visual>
  <collision>
    <origin xyz="0 0 -0.376" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://vehicle_4ws_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

