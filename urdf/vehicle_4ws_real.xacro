  <axis xyz="0.0 1.0 -0.0"/>
</joint>

<joint name="rear_to_Rsteering" type="fixed">
  <origin xyz="0.0 -0.4 -0.02" rpy="0 0 0"/>
  <parent link="v1_rear_1"/>
  <child link="new_hinge_2"/>
</joint>

<joint name="rear_to_Lsteering" type="fixed">
  <origin xyz="0.0 0.4 -0.02" rpy="0 0 0"/>
  <parent link="v1_rear_1"/>
  <child link="new_hinge_4"/>
</joint>

