<!-- ホイールのリンク設定 -->
<link name="new_wheel_1">
  <inertial>
    <origin xyz="3.1709373395083507e-05 2.540692010666312e-07 0.00021681028363659816" rpy="0 0 0"/>
    <mass value="4.639715839283939"/>
    <inertia ixx="0.029034" iyy="0.053334" izz="0.029034" ixy="-0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.450031 0.34248 -0.150215" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://vehicle_4ws_description/meshes/new_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="_"/>
  </visual>
  <collision>
    
    <!-- <origin xyz="-0.450031 0.34248 -0.150215" rpy="0 0 0"/> -->
    <!-- <geometry>
      <mesh filename="package://vehicle_4ws_description/meshes/new_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry> -->
    <origin xyz="0 0 0" rpy="1.57 0 0"/>
    <geometry>
      <cylinder radius="0.16" length="0.05"/>
    </geometry>

    <!-- ここから SDF の surface/friction -->
    <surface>
      <friction>
        <ode>
          <mu>${wheel_surface_mu1}</mu>
          <mu2>${wheel_surface_mu2}</mu2>
        </ode>
      </friction>
      <contact>
        <ode>
          <kp>${wheel_kp}</kp>
          <kd>${wheel_kd}</kd>
          <minDepth>${wheel_minDepth}</minDepth>
          <maxVel>${wheel_maxVel}</maxVel>
        </ode>
      </contact>
    </surface>
  </collision>
</link>

